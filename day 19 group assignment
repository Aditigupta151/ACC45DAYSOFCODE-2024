#include<iostream>
using namespace std;

int main(){
    
    int n;
    cout<<"enter number of people at the party:";
    cin>>n;
    int p;
    cout<<"preferred group size:";
    cin>>p;
    int arr[p];
    cout<<"enter member in group";
    for(int i =0;i<n;i++){
        cin>>arr[i];
        if(arr[i]>=0){
            int sum = arr[i];
        }
    }
    
}
