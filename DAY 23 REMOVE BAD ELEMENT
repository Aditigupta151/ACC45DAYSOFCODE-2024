#include<iostream>
using namespace std;

int main(){
    int size;
    cout<<"enter size of array";
    cin>>size;

    int arr[size];
    for(int i = 0;i<size;i++){
        cout<<"enter your element";
        cin>>arr[i];


    }

    // for checking values
    for(int i = 0;i<size;i++){
    if(arr[i]==arr[i+1])
        continue;
    
        
    return 0;
}
}
