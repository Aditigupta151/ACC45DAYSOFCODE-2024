N = int(input("enter N:"))
alice = int(input("enter between the number 1 or 2:"))
bob = int(input("enter between the number 1 or 2:"))

x =0


if(alice ==1):
    print(x= x+1)
elif(alice == 2):
    print(x = x+2)
else:
    print("invalid")

if(x>N):
    print("alice")

elif(x = N):
    print("bob")
else:

    if(bob== 1):
     
         print(x= x+1)
    elif(bob == 2):
         print(x = x+2)
    else:
        print("invalid")

if(x = N):
    print("bob")
else:
    print("error")

